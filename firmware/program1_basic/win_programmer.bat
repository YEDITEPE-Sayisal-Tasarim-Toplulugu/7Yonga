@echo off
rem Programmer settings
set PYTHON=python
set PY_FPGA_PROGRAMMER=../teknofest_uart_programmer_v1.py
set PROGRAMMER_HEX_FILENAME=BUILD/program.mem
set PROGRAMMER_PORT=COM8
