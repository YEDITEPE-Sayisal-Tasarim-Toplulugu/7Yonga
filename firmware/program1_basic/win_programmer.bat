set PROGRAMMER_DELAY=200

%PYTHON% %PY_FPGA_PROGRAMMER% --file %PROGRAMMER_HEX_FILENAME% --port %PROGRAMMER_PORT% --brate %PROGRAMMER_BAUDERATE% --delay %PROGRAMMER_DELAY%
